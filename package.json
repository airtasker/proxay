{
  "name": "proxay",
  "version": "1.3.0",
  "author": {
    "name": "Fran√ßois Wouts",
    "email": "f@zenc.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airtasker/proxay"
  },
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/cli.ts",
    "start:hot": "ts-node-dev --respawn src/cli.ts",
    "test": "jest --runInBand",
    "build": "rm -rf dist && tsc"
  },
  "bin": {
    "proxay": "./dist/cli.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "assert-never": "^1.2.1",
    "axios": "^0.20.0",
    "brotli": "^1.3.2",
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "deep-diff": "^1.0.2",
    "fs-extra": "^9.0.1",
    "js-yaml": "^3.14.0",
    "query-string": "^6.13.2",
    "string-similarity": "^4.0.2"
  },
  "devDependencies": {
    "@types/deep-diff": "^1.0.0",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.13",
    "@types/js-yaml": "^3.12.4",
    "@types/node": "^14.10.1",
    "@types/rimraf": "^3.0.0",
    "@types/string-similarity": "^3.0.0",
    "express": "^4.17.1",
    "jest": "^25.5.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
