{
  "name": "proxay",
  "version": "1.2.7",
  "author": {
    "name": "Fran√ßois Wouts",
    "email": "f@zenc.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zenclabs/proxay"
  },
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/cli.ts",
    "start:hot": "ts-node-dev --respawn src/cli.ts",
    "test": "jest --runInBand",
    "build": "rm -rf dist && tsc"
  },
  "bin": {
    "proxay": "./dist/cli.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "dependencies": {
    "@types/deep-diff": "^0.0.31",
    "@types/query-string": "^6.1.0",
    "assert-never": "^1.1.0",
    "axios": "^0.18.0",
    "brotli": "^1.3.2",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "deep-diff": "^1.0.2",
    "fs-extra": "^7.0.0",
    "js-yaml": "^3.12.0",
    "query-string": "^6.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.1",
    "@types/js-yaml": "^3.11.2",
    "@types/node": "^10.5.5",
    "express": "^4.16.3",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "typescript": "^3.0.1"
  }
}
